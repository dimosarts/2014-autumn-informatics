var a,P,chislit,znamenat:real;
    i:integer;
begin

chislit:=1;
znamenat:=1;

write('a=');read(a);

for i:=1 to 32 do 
    begin
    chislit:=chislit*(a-2*i);
    znamenat:=znamenat*(a-(2*i-1));
    end;
    
if znamenat<>0 then 
    begin
    P:=chislit/znamenat;  
    writeln('Результат=',P:1:2);
    end
else
    writeln('Деление на ноль');

end.
    
